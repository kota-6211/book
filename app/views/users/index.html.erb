<div class="container">
  <div class="row">
    <div class="col-3">
      <%= render "user", user: current_user %>
    </div>
    
    <div class="col-5">
      <% @users.each do |user| %>
       <%= image_tag user.get_profile(100, 100) %>
       <%= user.name %>
      <% end %>
    </div>
  </div>
  
</div>
